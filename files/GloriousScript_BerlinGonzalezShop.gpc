int red = 255, green = 0, blue = 0;
int colorStep = 1;

combo FullRainbow {
    if (red > 0 && green < 255 && blue == 0) { 
        red -= colorStep; 
        green += colorStep; 
    }
    else if (green > 0 && blue < 255 && red == 0) { 
        green -= colorStep; 
        blue += colorStep; 
    }
    else if (blue > 0 && red < 255 && green == 0) { 
        blue -= colorStep; 
        red += colorStep; 
    }

    set_rgb(red, green, blue);

    wait(20);
    combo_run(FullRainbow); 
}

